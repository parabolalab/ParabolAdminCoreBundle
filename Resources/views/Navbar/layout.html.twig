{# {% extends "AdmingeneratorGeneratorBundle:Navbar:layout.html.twig" %}
 #}
<nav class="navbar navbar-static-top" role="navigation">
    <!-- Sidebar toggle button-->
    <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </a>

    <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
            {% block navbar_list_container %}
                {% block navbar_profile %}
                    {% block navbar_profile_authenticated %}
                            <li class="user user-menu">
                                <a href="#" style="display: inline-block;">
                                    <i class="fa fa-user"></i>
                                    <span>{{ app.user ? app.user : 'profile.unauthenticated'|trans({}, 'Admingenerator') }} </span>
                                </a>
                                <a href="{{ path(admingenerator_config('logout_route')) }}" class=""  style="display: inline-block; text-transform: lowercase;">
                                        {{ 'profile.logout'|trans({}, 'Admingenerator') }}
                                    </a>
                            </li>
                    {% endblock navbar_profile_authenticated %}
                {% endblock navbar_profile %}
            {% endblock navbar_list_container %}
        </ul>
    </div>
</nav>


