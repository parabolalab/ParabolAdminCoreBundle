{% extends "AppAdminCoreBundle::base_admin_uncompressed.html.twig" %}

{% trans_default_domain "Admingenerator" %}

{% block page_header_content %}
        <h1>{% block page_header_icon %}<span class="{% block page_header_icon_class %}{% endblock page_header_icon_class %}" aria-hidden="true">{% endblock page_header_icon %}</span> {% block page_header_text %}{% endblock page_header_text %}</h1>
{% endblock page_header_content %}

{% block page_content %}
	<div class="row">
        <div class="col-md-12">
	        {% block page_content_box %}
	        	<div class="box box-primary">
		            <div class="box-body {% block page_content_box_body_class %}{% endblock page_content_box_body_class %}">	
						{% block page_content_box_body %}{% endblock page_content_box_body %}
					</div>
					<div class="box-footer {% block page_content_box_footer_class %}{% endblock page_content_box_footer_class %}">
	                    <div class="row">
	         				{% block page_content_box_footer %}{% endblock page_content_box_footer %}
	                    </div>
	                </div>
				</div>
			{% endblock page_content_box %}
		</div>
	</div>
{% endblock page_content %}