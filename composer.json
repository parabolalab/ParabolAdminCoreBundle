{
    "name": "parabol/parabol-admin-core-bundle",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "description": "CMS",
    "keywords": ["cms"],
    "homepage": "https://github.com/parabolalab/ParabolAdminCoreBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Kalota",
            "email": "marcin@parabolalab.com",
            "homepage": "http://www.parabolalab.com"
        }
    ],
    "require" : {
        "php": ">=5.6.0",
        "symfony2admingenerator/generator-bundle": "@dev"
    },
    "scripts": {
        "parabol-scripts": [
            "Parabol\\AdminCoreBundle\\Composer\\ScriptHandler::createAppBundles"
        ],
        "post-install-cmd": [
            "@parabol-scripts"
        ],
        "post-update-cmd": [
            "@parabol-scripts"
        ]
    },
    "autoload": {
        "psr-0": { "Parabol\\AdminCoreBundle": "", "App\\AdminCoreBundle": "" }
    },
    "target-dir": "Parabol/AdminCoreBundle"

}